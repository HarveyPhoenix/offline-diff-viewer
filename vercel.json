{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "serverMiddleware/**"
        ]
      }
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/v2",
      "permanent": false
    }
  ]
}
